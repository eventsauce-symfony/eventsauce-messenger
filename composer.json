{
    "name": "andreo/eventsauce-messenger",
    "description": "Symfony messenger for EventSauce message dispatcher.",
    "keywords": [
        "eventsauce",
        "components",
        "symfony",
        "messenger"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "eventsauce/eventsauce": "^3.0",
        "symfony/messenger": "^6.2",
        "symfony/dependency-injection": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "symfony/amqp-messenger": "^6.1",
        "roave/security-advisories": "dev-latest",
        "symfony/framework-bundle": "^6.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Messenger\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
